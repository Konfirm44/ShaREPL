@page "/"
@inject NavigationManager nav
@inject SharedEnvironmentProvider provider

<div class="text-align: center;">
    <button class="btn btn-primary" @onclick="Launch">Launch</button>
</div>

@code {
    void Launch()
    {
        var se = provider.GetSharedEnvironment();
        nav.NavigateTo(se.Guid.ToString());
    }
}